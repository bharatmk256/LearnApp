<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bharat.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Linear_Layout"
            android:label="LinearLayout" />
        <activity
            android:name=".Relative_Layout"
            android:label="RelativeLayout" />
        <activity
            android:name=".Constraint_layout"
            android:label="ConstraintLayout" />
        <activity
            android:name=".Scroll_View"
            android:label="ScrollView" />
        <activity
            android:name=".Card_View"
            android:label="CardView" />
        <activity
            android:name=".Java"
            android:label="Java" />
        <activity
            android:name=".Kotlin"
            android:label="Kotlin" />
    </application>

</manifest>